<core:FragmentDefinition 
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
   <SelectDialog
		id="mySelectDialog"
		noDataText="No Data!"
		search=".onSearch"
		confirm=".onDialogClose"
		cancel=".onDialogClose"
		items="{
			path: 'F4Model>/'
		}">
		<StandardListItem
			title="{F4Model>title}"
			description="{F4Model>description}"
            info="{F4Model>info}" 
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>