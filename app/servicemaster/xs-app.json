{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/user-api/attributes$",
      "target": "/attributes",
      "service": "sap-approuter-userapi"
    },
     {
      "source": "^/bpmworkflowruntime/v1/(.*)$",
      "target": "/public/workflow/rest/v1/$1",
      "destination": "BPA",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "/odata/v2/zmdgservice-master/(.*)$",
      "target": "/odata/v2/zmdgservice-master/$1",
      "destination": "mdg-service-dest-api",
      "csrfProtection": false,
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    }, 
    {
      "source": "^/sap/(.*)$",
      "target": "/sap/$1",
      "destination": "DIM",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}